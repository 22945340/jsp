<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<h1>form (input)</h1>
	<form>
		<!-- 텍스트 필드 -->
		<input />텍스트 필드 <br /> 
		
		<input type="text" />텍스트 필드<br />
		
		<!-- 수입력 필드 -->
		<input type="number" />수입력 필드<br />
		
		<!-- email 형식만 입력 -->
		<input type="email" />email 형식만 입력<br />
		
		<!-- 날짜 형식만 입력 -->
		<input type="date" /> 날짜 형식만 입력 <br />
		
		<!-- 날짜와 시간 형식 -->
		<input type="datetime-local" />날짜와 시간 형식 <br />
		
		<!-- 전송버튼 -->
		<input type="submit" value="전송버튼" /><br />
		
		<!-- 그냥버튼 -->
		<input type="button"  value="그냥 버튼" /><br />
		
		<!-- 전송버튼2 -->
		<button>전송버튼2</button> <br />
		
		<!-- 그냥버튼2 -->
		<button type="button">그냥버튼2</button><br />
		
	</form>
	

</body>
</html>